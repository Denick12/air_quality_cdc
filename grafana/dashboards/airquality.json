{
  "dashboard": {
    "id": null,
    "uid": "airquality_dashboard",
    "title": "Air Quality - Live",
    "tags": ["airquality", "streaming", "debezium", "kafka"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 8 },
        "type": "timeseries",
        "title": "PM2.5 (μg/m³)",
        "targets": [
          {
            "refId": "A",
            "format": "time_series",
            "datasource": null,
            "rawSql": "SELECT $__timeEpoch(current_time) as time, pm2_5 as value FROM air_quality WHERE $__timeFilter(current_time) ORDER BY current_time ASC"
          }
        ],
        "fieldConfig": { "defaults": {}, "overrides": [] }
      },
